{
    "version": 2,
    "builds": [
        {
            "src": "./index.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "/"
        }
    ],
    "crons": [
    {
      "path": "/api/v1/schedules/cron",
      "schedule": "1 * * * *"
    }
  ]
}
